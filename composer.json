{
  "name": "bjuppa/laravel-blog",
  "description": "Add blog functionality to your Laravel project",
  "keywords": [
    "laravel",
    "blog"
  ],
  "homepage": "https://github.com/bjuppa/laravel-blog",
  "license": "MIT",
  "authors": [
    {
      "name": "BjÃ¶rn Nilsved",
      "email": "nilsved@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.0",
    "illuminate/view": "~5.5.5|5.6.*",
    "illuminate/support": "5.5.*|5.6.*",
    "illuminate/database": "5.5.*|5.6.*",
    "illuminate/routing": "5.5.*|5.6.*",
    "illuminate/http": "5.5.*|5.6.*",
    "spatie/laravel-sluggable": "^2.1",
    "bjuppa/metatagbag": "^0.5"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5",
    "laravel/framework": ">=5.5.5",
    "phpunit/phpunit": ">6.5",
    "erusev/parsedown": "^1.7",
    "graham-campbell/markdown": "^10.0"
  },
  "suggest": {
    "graham-campbell/markdown": "Enables use of markdown in blog posts (install only one of the suggested markdown packages)",
    "erusev/parsedown": "Enables use of markdown in blog posts (install only one of the suggested markdown packages)"
  },
  "autoload": {
    "psr-4": {
      "Bjuppa\\LaravelBlog\\": "src/",
      "Bjuppa\\LaravelBlog\\Database\\Seeds\\": "database/seeds/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bjuppa\\LaravelBlog\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "post-update-cmd": "@clearCompiledViews",
    "clearCompiledViews": "rm -f vendor/orchestra/testbench-core/laravel/storage/framework/views/*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bjuppa\\LaravelBlog\\BlogServiceProvider"
      ],
      "aliases": {}
    }
  }
}
